  <body>
      <div class="container">
          <div class="row">
              <div class="col-1">
                <img src="./img/logo.png" alt="logo" width="120" height="120">

              </div>
          </div>
      </div>
    <div class="container" style="">
      <div class="row">
        <h2 class="text-center " >Your Location</h2>
        <input
          class="border border-primary"
          id="autocomplete"
          type="text"
          placeholder="Enter your location"
          style="height: 80px; margin:4%"
        />
        <button
          id="locationBtn"
          type="button"
          class="mt-2 mx-auto btn btn-danger text-center w-25"
        >
          Let's Go....
        </button>
      </div>
    </div>

    <!-- jQuery -->
    <script type="text/javascript" src="./js/jquery.min.js"></script>

    <!-- jquery ui -->
    <script src="./js/jquery-ui.js"></script>

